{
    "name": "snapserv/laravel-dotty",
    "description": "Dotty allows type-safe access to foreign nested data structures with static type hinting support",
    "type": "library",
    "homepage": "https://github.com/snapserv/laravel-dotty",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8 | ^9",
        "illuminate/http": "^9.45"
    },
    "require-dev": {
        "orchestra/testbench": "^7.17",
        "pestphp/pest": "^1.22",
        "vimeo/psalm": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "SnapServ\\Dotty\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SnapServ\\Dotty\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Pascal Mathis",
            "email": "dev@ppmathis.com"
        }
    ],
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "analyze": "vendor/bin/psalm",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SnapServ\\Dotty\\DottyServiceProvider"
            ],
            "aliases": {
                "Dotty": "SnapServ\\Dotty\\Dotty"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
